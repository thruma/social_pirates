<div class="top-bar">
    <div class="container">
        <div class="row">
            <div class="col">
                <div class="col-md-6 col-sm-12">
                    <h1><br><br></h1>
                    <form>
                        <h2><%= title %></h2>
                        <h1>Opret nyt medlems login!<br></h1>
                        <h3>Indtast medlems oplysninger:</h3>
                        <form action="/saveM" method="post">
                            <div class="form-group">
                                <label for="name">Fornavn:</label>
                                <input type="text" class="form-control" id="first_name" placeholder="Fornavn" name="first_name" required>
                            </div>
                            <div class="form-group">
                                <label for="name">Efternavn:</label>
                                <input type="text" class="form-control" id="last_name" placeholder="Efternavn" name="last_name" required>
                            </div>

                            <div class="form-group">
                                <label for="phone">Telefon nummer:</label>
                                <input type="text" class="form-control" id="telephone" placeholder="Telefon nummer" name="telephone" required>
                            </div>
                            <div class="form-group">
                                <label for="email">Email:</label>
                                <input type="email" class="form-control" id="email" placeholder="Enter email" name="email" required>
                            </div>
                            <div class="form-group">
                                <div class="form-group">
                                    <label for="password">Password:</label>
                                    <input type="text" class="form-control" id="password" placeholder="password" name="password" required>
                                </div>
                            </div>
                            <a href="#" class="btn btn-success" role="button" aria-pressed="true">Opret medlem!</a>
                            <a href="/frontend/index.html" class="btn btn-danger" role="button" aria-pressed="true">Tilbage</a>
                        </form>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>





<!-- jQuery -->
